"data","border","stripe","_component_el_table_column","default","props","_component_user_expand","user","row","prop","label","sortable","_createCommentVNode","fixed","width","scope","text","plain","_component_el_popconfirm","title","onConfirm","uid","reference","_component_el_pagination","onSizeChange","onCurrentChange","layout","total","_component_el_dialog","visible","footer","_component_el_form","model","_component_el_form_item","uname","upsw","_component_el_select","sex","_component_el_option","value","phone","city","email","_component_el_upload","action","_ctx","multiple","limit","tip","_hoisted_13"],"sourceRoot":"","sources":["D:\\Learning\\Y3S2\\Project\\Exc\\sprintboot-vue-demo\\vue\\src\\views\\UserEXM.vue"],"sourcesContent":["<template>\r\n  <div class=\"UserEXM\">\r\n    <div class=\"opeBoard\">\r\n      <el-button type=\"primary\" @click=\"addUser\">新增</el-button>\r\n      <el-button type=\"primary\" disabled>导入</el-button>\r\n      <el-button type=\"primary\" disabled>导出</el-button>\r\n    </div>\r\n    <div class=\"searchBoard\">\r\n      <el-input v-model=\"searchText\" placeholder=\"输入关键字\" style=\"width: 20%\" clearable/>\r\n      <el-button type=\"primary\" style=\"margin: 0 5px\"\r\n                 @click=\"searchName\">查询</el-button>\r\n    </div>\r\n    <div class=\"displayBoard\">\r\n      <el-table :data=\"tableData\"\r\n                border\r\n                stripe\r\n                style=\"width: 100%\"\r\n      >\r\n        <el-table-column type=\"expand\">\r\n          <!--        修改为自定义组件，显示其他信息-->\r\n          <template #default=\"props\">\r\n            <user-expand v-bind:user=\"props.row\" />\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"uid\"\r\n            label=\"ID\"\r\n            sortable/>\r\n        <el-table-column\r\n            prop=\"uname\"\r\n            label=\"姓名\"\r\n            sortable />\r\n        <!--     后期添加查看密码功能 -->\r\n        <el-table-column\r\n            prop=\"account\"\r\n            label=\"账户余额\" />\r\n        <el-table-column\r\n            prop=\"bank\"\r\n            label=\"银行账户\" />\r\n        <el-table-column fixed=\"right\" label=\"操作\" class=\"fixedOpe\" width=\"180px\">\r\n          <template #default=\"scope\">\r\n            <el-button text @click=\"editUser(scope.row)\" type=\"primary\" plain>编辑</el-button>\r\n            <el-popconfirm title=\"确认删除？\" @confirm=\"deleteUser(scope.row.uid)\">\r\n              <template #reference>\r\n                <el-button text @click=\"\" type=\"danger\">删除</el-button>\r\n              </template>\r\n            </el-popconfirm>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <!--    分页-->\r\n    <div style=\"margin: 10px\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n    <!--    新增用户弹窗-->\r\n    <el-dialog\r\n        v-model=\"addUserVisible\"\r\n        :title=userDialogTitle\r\n        :visible.sync=\"addUserVisible\"\r\n        width=\"800px\"\r\n    >\r\n      <el-form class=\"userForm\" :model=\"userForm\" >\r\n        <el-form-item class=\"formItem\" label=\"姓名\" label-width=\"100px\">\r\n          <el-input style=\"width: 120px\" v-model=\"userForm.uname\" />\r\n        </el-form-item>\r\n        <el-form-item class=\"formItem\" label=\"密码\" label-width=\"100px\">\r\n          <el-input style=\"width: 120px\" v-model=\"userForm.upsw\" />\r\n        </el-form-item>\r\n        <el-form-item class=\"formItem\" label-width=\"100px\" label=\"性别\">\r\n          <el-select v-model=\"userForm.sex\" style=\"width: 120px\">\r\n            <el-option label=\"男\" value=\"1\" />\r\n            <el-option label=\"女\" value=\"0\" />\r\n            <el-option label=\"保密\" value=\"-1\"/>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item class=\"formItem\" label-width=\"100px\" label=\"手机号\">\r\n          <el-input style=\"width: 120px\" v-model=\"userForm.phone\" />\r\n        </el-form-item>\r\n        <el-form-item class=\"formItem\" label-width=\"100px\" label=\"城市\">\r\n          <el-input style=\"width: 120px\" v-model=\"userForm.city\" />\r\n        </el-form-item>\r\n        <el-form-item class=\"formItem\" label-width=\"100px\" label=\"邮件\">\r\n          <el-input style=\"width: 200px\" v-model=\"userForm.email\" />\r\n        </el-form-item>\r\n        <!--    营业执照，身份证图片上传-->\r\n        <el-form-item class=\"formItem\" label-width=\"100px\" label=\"营业执照\">\r\n          <el-upload\r\n              class=\"upload-demo\"\r\n              action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n              :on-preview=\"handlePreview\"\r\n              :on-remove=\"handleRemove\"\r\n              :before-remove=\"beforeRemove\"\r\n              multiple\r\n              :limit=\"3\"\r\n              :on-exceed=\"handleExceed\"\r\n              :file-list=\"fileList\"\r\n          >\r\n            <el-button type=\"primary\">点击上传营业执照</el-button>\r\n            <template #tip>\r\n              <div class=\"el-upload__tip\" style=\"width: 200px\">\r\n                jpg/png 文件大小需小于 500KB.\r\n              </div>\r\n            </template>\r\n          </el-upload>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"addUserVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitUser\">提交</el-button>\r\n      </span>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport userExpand from \"@/components/UserExpand\";\r\nimport request from \"@/util/request\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"UserEXM\",\r\n  components: {\r\n    userExpand\r\n  },\r\n  data(){\r\n    return {\r\n      operate: \"\",\r\n      userDialogTitle: \"\",\r\n      searchText: \"\",\r\n      currentPage: 1,\r\n      pageSize: 5,\r\n      total: 10,\r\n      addUserVisible: false,\r\n      url:\"http://localhost:9090/\",\r\n      DataList:[],\r\n      tableData : [\r\n        {\r\n          id: 1,\r\n          name: 'yy',\r\n          psw: '123456',\r\n          status: '待审核'\r\n        },\r\n      ],\r\n      tableIndex:{\r\n        id: '',\r\n        name: '',\r\n        psw: '',\r\n        status: ''\r\n      },\r\n      userForm:{\r\n        uid:\"\",\r\n        name:\"\",\r\n        psw:\"\",\r\n        phone:\"\",\r\n        email:\"\",\r\n        city:\"\",\r\n        sex:\"\",\r\n        bank:\"\",\r\n        account:\"\",\r\n        point:\"\",\r\n        status:\"\",\r\n        license:\"\",\r\n        identity:\"\",\r\n        grade:\"\",\r\n        turnover:\"\",\r\n        likeRate:\"\",\r\n        MLikeRate:\"\",\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  filters:{\r\n    // userFmt(val){\r\n    //   switch (val){\r\n    //     case null:\r\n    //       return \"\";\r\n    //       break;\r\n    //     default:\r\n    //       return val;\r\n    //   }\r\n    // }\r\n  },\r\n  methods: {\r\n    load(){\r\n      let params={\r\n        pageNumber: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        searchText: this.searchText\r\n      }\r\n      request.get(\"/user\",{\r\n        params: params\r\n      })\r\n          .then(res=>{\r\n            console.log(res);\r\n            this.total=res.data.total;\r\n            this.tableData = res.data.records;\r\n            this.tableData.forEach(e=>{\r\n              for( var key in e){\r\n                if(e[key] === \"null\"){\r\n                  delete e[key];\r\n                }\r\n              }\r\n            })//过滤null\r\n          })\r\n    },\r\n    searchName(){\r\n      this.load();\r\n    },\r\n    addUser(){\r\n      this.addUserVisible = true;\r\n      this.userForm={};\r\n      this.operate=\"addUser\";\r\n      this.userDialogTitle=\"新增用户\"\r\n    },\r\n    editUser(row){\r\n      this.userForm = JSON.parse(JSON.stringify(row));\r\n      console.log(this.userForm)\r\n      this.addUserVisible = true;\r\n      this.operate=\"updateUser\"\r\n      this.userDialogTitle=\"编辑用户\"\r\n    },\r\n    submitUser(){\r\n      if (this.operate === \"addUser\"){\r\n        this.saveUser();\r\n      }else if (this.operate === \"updateUser\"){\r\n        this.updateUser();\r\n      }\r\n    },\r\n    updateUser(){\r\n      request.put(\"/user\",this.userForm).then(res=>{\r\n        // console.log(res)\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message: \"更新成功\",\r\n          });\r\n          this.addUserVisible = false;\r\n        }else{\r\n          this.$message({\r\n            type:\"error\",\r\n            message: res.msg,\r\n          })\r\n        }\r\n        this.load();\r\n      })\r\n    },\r\n    saveUser(){\r\n      request.post(\"/user\",this.userForm).then(res=>{\r\n        console.log(res)\r\n        this.addUserVisible = false;\r\n        this.load();\r\n      })\r\n    },\r\n    deleteUser(id){\r\n      console.log(id);\r\n      request.delete(\"/user/\"+id).then(res=>{\r\n\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message: \"删除成功\",\r\n          });\r\n          this.load()\r\n        }else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg,\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(val) {//改变每页的显示条数\r\n      this.pageSize = val;\r\n      this.load()\r\n    },\r\n    handleCurrentChange(val) {//改变页码\r\n      this.currentPage=val\r\n      this.load()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}