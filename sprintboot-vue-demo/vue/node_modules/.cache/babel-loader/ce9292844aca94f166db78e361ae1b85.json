emplate>\r\n          </el-image>\r\n        </div>\r\n      </el-col>\r\n      <el-col :span=\"15\" >\r\n        <div  class=\"goodInfo\">\r\n          <el-descriptions border column=\"2\">\r\n            <el-descriptions-item\r\n                label=\"类别\"\r\n                label-align=\"center\"\r\n                align=\"center\"\r\n                label-class-name=\"Item\"\r\n                width=\"150px\">\r\n              <span>{{ goodsForm.storage }}</span>\r\n            </el-descriptions-item>\r\n            <el-descriptions-item\r\n                label=\"好评率\"\r\n                label-align=\"center\"\r\n                align=\"center\"\r\n                label-class-name=\"Item\"\r\n                width=\"150px\">\r\n              <span>{{ goodsForm.likeRate }}</span>\r\n            </el-descriptions-item>\r\n            <el-descriptions-item\r\n                label=\"尺寸\"\r\n                label-align=\"center\"\r\n                align=\"center\"\r\n                label-class-name=\"Item\"\r\n                width=\"150px\">\r\n              <span>{{ goodsForm.size }}</span>\r\n            </el-descriptions-item>\r\n            <el-descriptions-item\r\n                label=\"是否可议价\"\r\n                label-align=\"center\"\r\n                align=\"center\"\r\n                label-class-name=\"Item\"\r\n                column-key=\"filterStatus\"\r\n                width=\"150px\">\r\n              <span v-if=\"goodsForm.bargain === 'true'\">可议价</span>\r\n              <span v-else>一口价</span>\r\n            </el-descriptions-item>\r\n            <el-descriptions-item\r\n                label=\"销量\"\r\n                label-align=\"center\"\r\n                align=\"center\"\r\n                label-class-name=\"Item\"\r\n                width=\"150px\">\r\n              <span>{{ goodsForm.sale }}</span>\r\n            </el-descriptions-item>\r\n            <el-descriptions-item\r\n                label=\"新旧程度\"\r\n                label-ali