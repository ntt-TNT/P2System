C,GAAR,CAAY5B,EAAZ;MACAjB,OAAO,CAAC2D,MAAR,CAAe,WAAS1C,EAAxB,EAA4B0B,IAA5B,CAAiC,aAAG,EAAE;QAEpC,IAAGG,GAAG,CAACc,IAAJ,KAAa,GAAhB,EAAoB;UAClB,MAAI,CAACC,QAAL,CAAc;YACZpB,IAAI,EAAC,SADO;YAEZqB,OAAO,EAAE;UAFG,CAAd;;UAIA,MAAI,CAACzB,IAAL;QACD,CAND,MAMM;UACJ,MAAI,CAACwB,QAAL,CAAc;YACZpB,IAAI,EAAE,OADM;YAEZqB,OAAO,EAAEhB,GAAG,CAACiB;UAFD,CAAd;QAIF;MACD,CAdD;IAeD,CAlEM;IAmEPC,gBAnEO,4BAmEUC,GAnEV,EAmEe;MAAC;MACrB,KAAKtD,QAAL,GAAgBsD,GAAhB;MACA,KAAK5B,IAAL;IACD,CAtEM;IAuEP6B,mBAvEO,+BAuEaD,GAvEb,EAuEkB;MAAC;MACxB,KAAKvD,WAAL,GAAiBuD,GAAjB;MACA,KAAK5B,IAAL;IACD,CA1EM;IA2EP8B,WA3EO,uBA2EKZ,GA3EL,EA2ES;MACdA,GAAG,CAACJ,SAAJ,GAAgB,KAAhB;IACD,CA7EM;IA8EPiB,YA9EO,wBA8EMb,GA9EN,EA8EU;MACfA,GAAG,CAACJ,SAAJ,GAAgB,MAAhB;IACF;EAhFO,CAxDI;EA0IbkB,OAAO,EAAC;AA1IK,CAAf","names":["userExpand","request","Delete","axios","name","components","data","statusSelect","userDialogTitle","searchText","searchSelect","currentPage","pageSize","total","addUserVisible","url","DataList","tableData","id","psw","status","tableIndex","userForm","uid","phone","email","city","sex","bank","account","point","license","identity","grade","turnover","likeRate","MLikeRate","created","load","methods","params","pageNumber","type","get","then","console","log","res","records","forEach","key","e","iconColor","String","search","agreeUser","row","put","refuseUser","deleteUser","delete","code","$message","message","msg","handleSizeChange","val","handleCurrentChange","deleteToRed","deleteToGrey","filters"],"sourceRoot":"","sources":["D:\\Learning\\Y3S2\\Project\\Exc\\sprintboot-vue-demo\\vue\\src\\views\\UserEXM.vue"],"sourcesContent":["<template>\r\n  <div class=\"UserEXM\">\r\n    <div class=\"opeBoard\">\r\n      <el-button type=\"primary\" @click=\"load\">刷新</el-button>\r\n    </div>\r\n    <div class=\"searchBoard\">\r\n      <el-select v-model=\"searchSelect\" slot=\"prepend\" placeholder=\"请选择搜索对象\" style=\"width: 100px\">\r\n        <el-option label=\"ID\" value=\"uid\"></el-option>\r\n        <el-option label=\"姓名\" value=\"uname\"></el-option>\r\n      </el-select>\r\n      <el-input v-model=\"searchText\" placeholder=\"输入搜索内容\" style=\"width: 20%\" clearable>\r\n      </el-input>\r\n      <el-button type=\"primary\" style=\"margin: 0 5px\"\r\n                 @click=\"search\">查询</el-button>\r\n    </div>\r\n    <div class=\"displayBoard\">\r\n      <el-table :data=\"tableData\"\r\n                border\r\n                stripe\r\n                @filter-change=\"filterStatus\"\r\n                style=\"width: 100%\"\r\n      >\r\n        <el-table-column type=\"expand\">\r\n          <!--        修改为自定义组件，显示其他信息-->\r\n          <template #default=\"props\">\r\n            <user-expand v-bind:user=\"props.row\" />\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"uid\"\r\n            label=\"ID\"\r\n            sortable/>\r\n        <el-table-column\r\n            prop=\"uname\"\r\n            label=\"姓名\"\r\n            sortable />\r\n        <!--     后期添加查看密码功能 -->\r\n        <el-table-column\r\n            label=\"状态\"\r\n        >\r\n          <template #default=\"scope\">\r\n            <span v-if=\"scope.row.status === 0\" style=\"color: red;\">不通过</span>\r\n            <span v-if=\"scope.row.status === 1\" style=\"color: darkgreen;\">通过</span>\r\n            <span v-if=\"scope.row.status === 3\" style=\"color: dodgerblue;\">审核中</span>\r\n            <span v-if=\"scope.row.status === 4\" style=\"color: red;\">黑名单</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"操作\" class=\"fixedOpe\" width=\"180px\">\r\n          <template #default=\"scope\">\r\n            <el-button text @click=\"agreeUser(scope.row)\" type=\"primary\" plain>通过</el-button>\r\n            <el-button text @click=\"refuseUser(scope.row)\" type=\"danger\">拒绝</el-button>\r\n            <el-popconfirm title=\"确认删除？\" @confirm=\"deleteUser(scope.row.uid)\">\r\n              <template #reference>\r\n                <el-icon\r\n                    @mouseover=\"deleteToRed(scope.row)\"\r\n                    @mouseout=\"deleteToGrey(scope.row)\"\r\n                    :style=\"{color:scope.row.iconColor}\"\r\n                    class=\"deleteIcon\"\r\n                    slot=\"suffix\"><Delete /></el-icon>\r\n              </template>\r\n            </el-popconfirm>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <!--    分页-->\r\n    <div style=\"margin: 10px\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport userExpand from \"@/components/UserExpand\";\r\nimport request from \"@/util/request\";\r\n\r\nimport {Delete} from \"@element-plus/icons-vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"UserEXM\",\r\n  components: {\r\n    userExpand,\r\n    Delete\r\n  },\r\n  data(){\r\n    return {\r\n      statusSelect: \"\",\r\n      userDialogTitle: \"\",\r\n      searchText: \"\",\r\n      searchSelect: \"uname\",\r\n      currentPage: 1,\r\n      pageSize: 5,\r\n      total: 10,\r\n      addUserVisible: false,\r\n      url:\"http://localhost:9090/\",\r\n      DataList:[],\r\n      tableData : [\r\n        {\r\n          id: 1,\r\n          name: 'yy',\r\n          psw: '123456',\r\n          status: '待审核'\r\n        },\r\n      ],\r\n      tableIndex:{\r\n        id: '',\r\n        name: '',\r\n        psw: '',\r\n        status: ''\r\n      },\r\n      userForm:{\r\n        uid:\"\",\r\n        name:\"\",\r\n        psw:\"\",\r\n        phone:\"\",\r\n        email:\"\",\r\n        city:\"\",\r\n        sex:\"\",\r\n        bank:\"\",\r\n        account:\"\",\r\n        point:\"\",\r\n        status:\"\",\r\n        license:\"\",\r\n        identity:\"\",\r\n        grade:\"\",\r\n        turnover:\"\",\r\n        likeRate:\"\",\r\n        MLikeRate:\"\",\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load(){\r\n      let params={\r\n        pageNumber: this.currentPage,\r\n        pageSize: this.pageSize,\r\n        searchText: this.searchText,\r\n        type: this.searchSelect\r\n      }\r\n      request.get(\"/exmU\",{\r\n        params: params\r\n      })\r\n      .then(res=>{\r\n            console.log(res);\r\n            this.total=res.data.total;\r\n            this.tableData = res.data.records;\r\n            this.tableData.forEach(e=>{\r\n              for( var key in e){\r\n                if(e[key] === \"null\"){\r\n                  delete e[key];\r\n                }\r\n              }//过滤null\r\n              e.iconColor='grey'\r\n              e.sex = String(e.sex)\r\n            })\r\n          })\r\n    },\r\n    search(){\r\n      this.load();\r\n    },\r\n    agreeUser(row){\r\n      this.userForm = row\r\n      console.log(this.userForm)\r\n      request.put(\"/exmU/\"+1,this.userForm).then(res=>{\r\n        console.log(res)\r\n        this.userForm={}\r\n        this.addUserVisible = false;\r\n        this.load();\r\n      })\r\n    },\r\n    refuseUser(row){\r\n      this.userForm = row\r\n      console.log(this.userForm)\r\n      request.put(\"/exmU/\"+0,this.userForm).then(res=>{\r\n        console.log(res)\r\n        this.userForm={}\r\n        this.addUserVisible = false;\r\n        this.load();\r\n      })\r\n    },\r\n    deleteUser(id){\r\n      console.log(id);\r\n      request.delete(\"/exmU/\"+id).then(res=>{\r\n\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message: \"删除成功\",\r\n          });\r\n          this.load()\r\n        }else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg,\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(val) {//改变每页的显示条数\r\n      this.pageSize = val;\r\n      this.load()\r\n    },\r\n    handleCurrentChange(val) {//改变页码\r\n      this.currentPage=val\r\n      this.load()\r\n    },\r\n    deleteToRed(row){\r\n      row.iconColor = 'red'\r\n    },\r\n    deleteToGrey(row){\r\n      row.iconColor = 'grey'\r\n    }\r\n  },\r\n  filters:{\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.UserEXM{\r\n  padding: 10px;\r\n}\r\n.opeBoard{\r\n  margin: 10px 0;\r\n}\r\n.searchBoard{\r\n  margin: 10px 0;\r\n}\r\n.fixedOpe{\r\n  width: 120px;\r\n  text-align: center;\r\n}\r\n\r\n.deleteIcon{\r\n  margin-left: 8px;\r\n}\r\n</style>"]}]}