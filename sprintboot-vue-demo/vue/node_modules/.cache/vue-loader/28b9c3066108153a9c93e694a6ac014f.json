riptions-item\r\n          label=\"好评率\"\r\n          label-align=\"center\"\r\n          align=\"center\"\r\n          label-class-name=\"Item\"\r\n          width=\"150px\">{{ goodsForm.likeRate }}</el-descriptions-item>\r\n      <el-descriptions-item\r\n          label=\"商品介绍\"\r\n          label-align=\"center\"\r\n          align=\"center\"\r\n          label-class-name=\"Item\"\r\n          width=\"150px\">{{ goodsForm.introduction }}</el-descriptions-item>\r\n    </el-descriptions>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { toRaw } from '@vue/reactivity';\r\n\r\nexport default {\r\n  name: \"goodsExpand\",\r\n  props:{\r\n    goods:Object,\r\n  },\r\n  // props:[\"user\"],\r\n  components:{},\r\n  data() {\r\n    return {\r\n      goodsForm:{\r\n        gid:\"\",\r\n        gname:\"\",\r\n        type:\"\",\r\n        price:\"\",\r\n        size:\"\",\r\n        picture:\"\",\r\n        introduction:\"\",\r\n        bargain:\"\",\r\n        storage:\"\",\r\n        gcondition:\"\",\r\n        mid:\"\",\r\n        oid:\"\",\r\n        sale:\"\",\r\n        likeRate:\"\",\r\n        status:\"\",\r\n      },\r\n      canBargain: \"true\"\r\n    }\r\n  },\r\n  created() {\r\n    this.onload()\r\n  },\r\n  methods:{\r\n    onload(){\r\n      this.goodsForm=toRaw(this.goods);\r\n      console.log(this.goodsForm)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#goodsExpand{\r\n  display: block;\r\n}\r\n.Item{\r\n  display: inline-block;\r\n  min-width: 100px;\r\n  min-height: 50px;\r\n  line-height: 50px;\r\n  text-align: left;\r\n\r\n}\r\n</style>"]}]}